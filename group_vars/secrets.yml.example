---
# EXAMPLE: Copy this file to secrets.yml and update with your actual values
# This file can be safely committed to version control

# MinIO Credentials
minio_root_user: "minioadmin"
minio_root_password: "CHANGE_ME_IN_PRODUCTION"

# Argo CD Admin Password
# If not set, a random password will be generated and displayed during installation
argocd_admin_password: "CHANGE_ME_IN_PRODUCTION"

# Argo CD Repository Configuration (optional)
# Uncomment and configure these variables to enable app-of-apps pattern
# argocd_repo_url: "https://github.com/your-username/your-apps-repo.git"
# argocd_repo_branch: "main"
# argocd_repo_path: "apps"

# If your Argo CD repository requires authentication, add credentials here:
# argocd_repo_username: "your-username"
# argocd_repo_password: "your-token-or-password"
